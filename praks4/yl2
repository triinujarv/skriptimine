#!/bin/bash

echo -n "Sisesta aluskandiku pikkus: "
read pikkus
echo -n "Sisesta aluskandiku laius: "
read laius
kandik=`expr $pikkus \* $laius`
echo $kandik

echo -n "Sisesta k端psiste pikkus: "
read pikkus
echo -n "Sisesta k端psiste laius: "
read laius
kypsis=`expr $pikkus \* $laius`
echo $kypsis

echo -n "Sisesta kihtide arv: "
read kihtide_arv
echo -n "Sisesta k端psiste arv 端hes pakis: "
read kypsiste_arv
pakkide_arv=`expr $kandik / $kypsis \* $kihtide_arv / $kypsiste_arv`
echo $pakkide_arv
